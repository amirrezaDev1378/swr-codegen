{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"lib": ["ESNext"],
		"allowJs": true,
		"outDir": "dist",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "/example", "/dist", "temp"]
}
