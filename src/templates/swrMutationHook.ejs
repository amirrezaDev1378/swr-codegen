export function use<%= queryName %>(variables: <%- queryVariables %>) {
    const {mutation: mutationFetcher} = fetcher;

    const {data, error, trigger, reset, isMutating} = useSWRMutation<<%- responseType %>>({query: `<%= query %>`, variables}, mutationFetcher);

    return {
        data,
        error,
        trigger,
        reset,
        isMutating
    };
}
