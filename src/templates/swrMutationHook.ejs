export function use<%= queryName %>(variables?: <%- queryVariables %> , options?: SWRMutationConfiguration<<%- responseType %>, any, any, <%- queryVariables %>, <%- responseType %>>) {
    if (!options) options = {};
    const {
        data,
        error,
        trigger,
        reset,
        isMutating
    } = useSWRMutation<<%- responseType %> , any, any, <%- queryVariables %>, <%- responseType %>>({
            query: `<%= query %>`,
            variables
        } as any
        , fetcher as any,
        {revalidate: false, ...(options as any)});

    return {
        data,
        error,
        trigger,
        reset,
        isMutating
    };
}
