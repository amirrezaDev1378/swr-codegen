export function use<%= queryName %>(handleTrigger:MutationFetcher<<%- responseType %>, any, <%- queryVariables %>> , options?: SWRMutationConfiguration<<%- responseType %>, any, any, <%- queryVariables %>, CreateMessageMutation>) {

    const {data, error, trigger, reset, isMutating} = useSWRMutation<<%- responseType %> , any , any , <%- queryVariables %>, <%- responseType %>>({query: `<%= query %>`}, handleTrigger , options);

    return {
        data,
        error,
        trigger,
        reset,
        isMutating
    };
}




