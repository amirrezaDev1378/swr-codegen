


export function use<%= queryName %>(variables: <%- queryVariables %>) {
  const {bare: bareFetcher} = fetcher;


  const { data, error , isLoading, isValidating, mutate } = useSwr<<%- responseType %>>({ query: `<%= query %>`, variables}, bareFetcher);

  return {
    data,
    error,
    isLoading,
    isValidating,
    mutate
  }
}
